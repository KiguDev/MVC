@model RestauranteViewModel
@{
    ViewData["Title"] = Model.Nombre;
}

<div class="container">
    <div class="text-center">
        <h1>@Model.Nombre</h1>
        <h2 class="text-center">@Model.Direccion</h2>
    </div>

    <div class="row">
        <div class="col-md-6">
            <div>Cierra a las: @Model.HoraDeCierre horas</div>
            <div></div>
        </div>
        <div class="col-md-6">
            @Model.
        </div>
    </div>
</div>

